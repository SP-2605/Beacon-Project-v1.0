{
  "name": "beacon",
  "version": "1.0.0",
  "description": "SIESGST BEACON",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/styles.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/styles.concat.css -o css/styles.prefix.css",
    "compress:css": "node-sass css/styles.prefix.css css/styles.css --output-styles compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "SURIYA.S",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "imports-loader": "^0.8.0",
    "javascript": "^1.0.0",
    "jquery": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SP-2605/Beacon-Project-v1.0.git"
  },
  "bugs": {
    "url": "https://github.com/SP-2605/Beacon-Project-v1.0/issues"
  },
  "homepage": "https://github.com/SP-2605/Beacon-Project-v1.0#readme"
}
